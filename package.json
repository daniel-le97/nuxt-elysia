{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun --bun nuxt build",
    "dev:nuxt": "bun --bun nuxt dev",
    "dev:elysia": "bun --bun --watch index.ts",
    "dev": "bun run kill && concurrently \"bun run dev:nuxt\" \"bun run dev:elysia\"",
    "generate": "nuxt generate",
    "preview": "bun --bun nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --fix",
    "kill": "lsof -t -i:5566 | xargs kill && lsof -t -i:3000 | xargs kill"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.3",
    "@nuxt/devtools": "latest",
    "@nuxt/image": "latest",
    "@nuxt/ui": "latest",
    "bun-types": "^1.0.11",
    "concurrently": "^8.2.2",
    "elysia": "^0.7.25",
    "eslint": "^8.53.0",
    "nuxt": "^3.8.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@elysiajs/eden": "^0.7.4",
    "@elysiajs/stream": "^0.7.2",
    "@nuxt/content": "^2.9.0",
    "giget": "^1.1.3"
  }
}
